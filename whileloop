package com.Anshu;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        //print numbers from 1 to 10
        int num = 1;
        while(num <= 10){
            System.out.println((num));
            num += 1;
        }
    }
}

